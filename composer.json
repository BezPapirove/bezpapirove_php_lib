    "require": {
        "ramsey/uuid": "5.x-dev",
        "php": ">=8.0",
        "guzzlehttp/guzzle": "8.0.x-dev",
        "guzzlehttp/psr7": "2.6.x-dev",
        "tecnickcom/tcpdf": "dev-main",
        "setasign/fpdi": "dev-master",
        "ext-openssl": "*"
